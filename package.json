{
  "name": "project-sso",
  "version": "1.0.0",
  "description": "",
  "main": "main/main.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "start:debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 -r source-map-support/register ./dist/main/server.js --nolazy ./dist/main/server.js",
    "start:dev": "sucrase-node src/main/server.ts",
    "test": "jest",
    "prepare": "husky install",
    "build:compile": "swc src -d dist --source-maps --config-file .swcrc -s inline",
    "build": "rimraf dist && npm run build:compile"
  },
  "author": "Rafael Cavalcante <ra.facavalcante@hotmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.68",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "sucrase": "^3.32.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
